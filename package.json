{
  "name": "alephbet",
  "version": "0.20.1",
  "author": "Yoav Aner <yoav@gingerlime.com>",
  "files": [
    "lib",
    "dist",
    "src",
    "vendor",
    "test"
  ],
  "browserify": {
    "transform": [
      "coffeeify"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "coffee"
    ],
    "testRegex": "/test/.+_test\\.coffee?",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.coffee$": "<rootDir>/test/preprocessor.js"
    }
  },
  "dependencies": {
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.12.7",
    "babel-jest": "^26.6.3",
    "coffee-loader": "^1.0.1",
    "coffeescript": "^2.5.1",
    "coffeetape": "^2.0.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0"
  },
  "scripts": {
    "prepare": "webpack --config webpack.config.coffee --mode development",
    "prepack": "webpack --config webpack.config.coffee --mode production",
    "analyze": "webpack --config webpack.config.coffee --mode production --analyze",
    "test": "jest --verbose",
    "watch": "jest --watch",
    "debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "description": "AlephBet is a pure-javascript A/B (multivariate) testing framework for developers.",
  "main": "dist/alephbet.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alephbet/alephbet.git"
  },
  "keywords": [
    "A/B Testing",
    "Multivariate",
    "Browser",
    "Browserify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Alephbet/alephbet/issues"
  },
  "homepage": "https://github.com/Alephbet/alephbet"
}
